{
  "name": "RojoTestGame",
  "tree": "./src",
  "assets": "./assets",
  "config": {
    "extractBinaryAssets": true,
    "binaryAssetTypes": [
      "Animation",
      "Mesh",
      "Sound",
      "Image"
    ],
    "excludeServices": [
      "CoreGui",
      "RobloxPluginGuiService",
      "CorePackages"
    ],
    "excludeClasses": [],
    "scriptSourceMode": "external",
    "terrainMode": "voxelData",
    "csgMode": "assetReference",
    "chunkSize": 1000,
    "generateToolingFiles": true
  },
  "sync": {
    "mode": "bidirectional",
    "conflictResolution": "prompt",
    "autoSync": false,
    "watchPaths": [
      "./src"
    ]
  },
  "treeMapping": {
    "StarterGui": "src/ui",
    "ServerScriptService": "src/server",
    "StarterPlayer/StarterPlayerScripts": "src/client",
    "ReplicatedStorage/Shared": "src/shared",
    "ReplicatedStorage/Packages": "Packages",
    "Workspace/Maps": "src/maps"
  }
}